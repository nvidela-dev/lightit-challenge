{
  "name": "patient-registration-api",
  "version": "1.0.0",
  "description": "Patient registration API with Express, Prisma, and BullMQ",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc && tsc-alias",
    "start": "node dist/index.js",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "test": "vitest run",
    "test:watch": "vitest",
    "worker": "tsx src/workers/notification.worker.ts",
    "worker:dev": "tsx watch src/workers/notification.worker.ts"
  },
  "dependencies": {
    "@prisma/client": "^6.3.1",
    "bullmq": "^5.34.8",
    "cors": "^2.8.5",
    "dotenv": "^17.2.4",
    "express": "^4.21.2",
    "multer": "^2.0.2",
    "nodemailer": "^8.0.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.13.1",
    "@types/nodemailer": "^6.4.17",
    "@types/supertest": "^6.0.3",
    "prisma": "^6.3.1",
    "supertest": "^7.2.2",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "vitest": "^3.2.4"
  }
}
